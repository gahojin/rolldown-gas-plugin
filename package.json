{
  "name": "@gahojin-inc/rolldown-gas-plugin",
  "version": "0.6.0",
  "description": "Rolldown plugin for Google App Script",
  "author": "GAHOJIN, Inc.",
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/gahojin/rolldown-gas-plugin.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "rolldown",
    "gas",
    "google-app-script"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "postinstall": "biome migrate --write",
    "build": "rolldown -c",
    "prepublishOnly": "pnpm run build",
    "lint": "biome check --write .",
    "check": "tsgo",
    "check:packagejson": "pnpm attw --profile esm-only --pack .",
    "test": "vitest --watch",
    "test:unit": "vitest --passWithNoTests --run --coverage",
    "test:update": "vitest --update",
    "bench": "vitest bench",
    "ncu": "npm-check-updates -u"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@biomejs/biome": "2.3.14",
    "@fast-check/vitest": "0.2.4",
    "@types/node": "20.19.33",
    "@typescript/native-preview": "7.0.0-dev.20260209.1",
    "@vitest/coverage-v8": "4.0.18",
    "jest-extended": "7.0.0",
    "npm-check-updates": "19.3.2",
    "rolldown": "1.0.0-rc.3",
    "typescript": "5.9.3",
    "unplugin-isolated-decl": "0.15.7",
    "vite": "7.3.1",
    "vite-tsconfig-paths": "6.1.0",
    "vitest": "4.0.18"
  }
}
