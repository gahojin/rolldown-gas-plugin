{
  "name": "@gahojin-inc/rolldown-gas-plugin",
  "version": "0.4.2",
  "description": "Rolldown plugin for Google App Script",
  "author": "GAHOJIN, Inc.",
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/gahojin/rolldown-gas-plugin.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "rolldown",
    "gas",
    "google-app-script"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "rolldown -c",
    "prepublishOnly": "pnpm run build",
    "lint": "biome check --write .",
    "check": "tsc",
    "check:packagejson": "pnpm attw --profile node16 --pack .",
    "test": "vitest --watch",
    "test:unit": "vitest --passWithNoTests --run --coverage",
    "bench": "vitest bench",
    "ncu": "npm-check-updates -u"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.cts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "require": "./dist/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@biomejs/biome": "2.3.8",
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@fast-check/vitest": "0.2.4",
    "@types/node": "20.19.26",
    "@vitest/coverage-v8": "4.0.15",
    "jest-extended": "7.0.0",
    "lefthook": "2.0.9",
    "npm-check-updates": "19.2.0",
    "rolldown": "1.0.0-beta.53",
    "typescript": "5.9.3",
    "unplugin-isolated-decl": "0.15.6",
    "vite": "7.2.7",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.15"
  }
}
